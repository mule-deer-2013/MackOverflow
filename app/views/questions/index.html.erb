Welcome to MackOverflow!

<% unless logged_in? %>
<div>
  <%= link_to "Sign Up!", new_user_path %>
</div>
<% end %>

<div class="question_container">
<% @questions.each do |question| %>

	<div class="question">

    <%= render partial: "vote_box", locals: {:votable => question} %>
    <div class="qtitle">
      <%= link_to question.content, question %>
    </div>
    <span class="author"> Author: <%= link_to question.user.username, question.user %></span>
  </div>

<% end %>
</div>
