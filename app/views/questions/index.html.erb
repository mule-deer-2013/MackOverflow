Welcome to MackOverflow!

<div>
  <%= link_to "Sign Up!", new_user_path %>
</div>


<% @questions.each do |question| %>

	<div class="question">

    <%= render partial: "vote_box", locals: {:question => question} %>
    <div class="qtitle">
      <%= link_to question.content, question %>
    </div>
  </div>

<% end %>



